{
  "allowCommands": [
    "git init -b main",
    "git add . && git commit -m \"<msg>\"",
    "git push",
    "npm ci",
    "npm install",
    "npm run build --workspaces",
    "npx supabase --version",
    "npx supabase login",
    "npx supabase link --project-ref <ref>",
    "npx supabase db push",
    "npx supabase functions deploy <name>",
    "npx supabase secrets set --env-file <path>"
  ],
  "notes": "Only use these commands. Ask before any other command.",
  "supabase": {
    "region": "eu",
    "edgeFunctionsRuntime": "deno",
    "linking": "use project ref",
    "authProviders": ["google", "azure"]
  }
}

